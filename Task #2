#include <stdio.h>

int main()
{
int pow(int x, int p) {
	int i,res=x;
	if (p == 0)
		return 1;

	for (i = 1; i < p; i++) {
		res = res*x;
	}
	return res;
}

void binary() {
	int n;
	int i = 0, res = 0;

	scanf("%i", &n);

	while (n > 0) {
		res += (n % 10)*pow(2, i);
		n = n / 10;
		i++;
	}
	printf("%i\n\n", res);
}
return 0;
}
