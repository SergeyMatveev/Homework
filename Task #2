#include <stdio.h>
#include <math.h>


void main()
{
do{
    int n = 0;
    int i = 0;
    int res = 0;
    int check = 0;

    scanf("%d", &n);

    printf("val = %i\n", n);

    while (n > 0)
    {
        check = (n % 10);
        if (check == 1 || check ==0)
        {
            res += check*pow(2, i);
            n = n / 10;
            i++;
        }
        else{
            printf("Not a binary\n");
            break;
        }
    }
    printf("%d\n\n", res);

}
while(1);


    return;
}
